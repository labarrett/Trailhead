<!DOCTYPE html>
<html>
<head>
  <title>Views</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
  <script src="https://use.typekit.net/spu8mfr.js"></script>
<script>try{Typekit.load({ async: true });}catch(e){}</script>
</head>
<body>

  <nav class="navbar navbar-inverse navbar-fixed-top color">
     <div class="container">
       <div class="navbar-header">
         <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar" aria-expanded="false" aria-controls="navbar">
           <span class="sr-only">Toggle navigation</span>
           <span class="icon-bar"></span>
           <span class="icon-bar"></span>
           <span class="icon-bar"></span>
         </button>
         <%= link_to "THE VIEWS", root_path, class: "navbar-brand colortwo" %>
       </div>

       <div id="navbar" class="collapse navbar-collapse">
             <ul class="nav navbar-nav">

             </ul>

         <ul class="nav navbar-nav navbar-right colortwo">
           <% if user_signed_in?%>
              <li><%= link_to 'Pins', pins_path %></li>
           <li><%= link_to 'Profile',edit_user_registration_path %></li>
          <li> <%= link_to 'Logout', destroy_user_session_path, method: :delete %></li>


           <% else %>
           <li><%= link_to 'Login', new_user_session_path %></li>
           <li><%= link_to 'Sign Up', new_user_registration_path %></li>
           <% end%>
         </ul>
       </div>
     </div>
   </nav>



  <!--<p class="notice"><%= notice %></p>
     <p class="alert"><%= alert %></p>-->

<%= yield %>


<footer class ="footer">


</footer>
</body>
</html>
